#include <iostream>
using namespace std;
int main () {
 //que 1
    int x=2;
    int y=4;
    cout<<x*y<<endl;
//que 2
    cout<<"ascii valuue of u: "<<(int)'U'<<endl;
//que 3
    int l=7;
    int b=4;
    int area=l*b;
    cout <<area<<endl;
//que 4
    int a=4;
    int cube=a*a*a;
    cout <<area<<endl;
//que 5
     cout<<"the size of data type is"<<endl;
     cout<<"(char): "<<sizeof(char)<<endl;
     cout<<"(short): "<<sizeof(short)<<endl;
     cout<<"(int): "<<sizeof(int)<<endl;
     cout<<"(long): "<<sizeof(long)<<endl;
     cout<<"(long long): "<<sizeof(long long)<<endl;
     cout<<"(float): "<<sizeof(float)<<endl;
     cout<<"(double): "<<sizeof(double)<<endl;
     cout<<"(long double): "<<sizeof(long double)<<endl;
     
     cout<<"(bool): "<<sizeof(bool)<<endl;

//que 6
    int u=2;
    int v=3;
    int w;
    w=u;
    u=v;
    v=w;
    
    cout<<"u = "<<u<<endl;
    cout<<"v = "<<v<<endl;

    return 0;
}

